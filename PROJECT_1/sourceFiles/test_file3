/*## INPUT ##*/
 Rational add(const Rational &a,const Rational &b){    
    double num1,den1;
      
    num1 = ((a.num *b.den) + (a.den*b.num));
    den1 =(a.den*b.den);
       
    return Rational(num1,den1);        
}


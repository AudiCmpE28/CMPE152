/*## INPUT ##*/
{    
    int b; b = 1;    
    {        
        int a; a = 2;        
        do a = a+1; while(a < 100);
    }
}


/*## OUTPUT-RESULTS: ##*/
{   {
BASE_TYPE   int
ID   b
;   ;
ID   b
=   =
NUM   1
;   ;
{   {
BASE_TYPE   int
ID   a
;   ;
ID   a
=   =
NUM   2
;   ;
DO   do
ID   a
=   =
ID   a
+   +
NUM   1
;   ;
WHILE   while
(   (
ID   a
<   <
NUM   100
)   )
;   ;
}   }
}   }